@using Microsoft.AspNetCore.Components.Web
@using BootstrapBlazor.Components

<h1>Counter</h1>

<p>The current count is: @currentCount</p>
<button @onclick="IncrementCount">Count+5</button>

<Button Color="Color.Primary">主要按钮</Button>

@code {
    int currentCount = 0;

    void IncrementCount()
    {
        currentCount+=5;
    }
}